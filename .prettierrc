{
  "endOfLine": "auto",
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "no-multi-spaces": "error",
  "space-in-parens": "error",
  "no-multiple-empty-lines": "error",
  "prefer-const": "error",
  "no-use-before-define": "error"
}
